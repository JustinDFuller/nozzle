{
    "version": "2",
    "run": {
        "go": "1.23.1",
        "modules-download-mode": "readonly"
    },
    "output": {
        "show-stats": true,
        "sort-order": ["file"]
    },
    "linters": {
        "default": "all",
        "disable": [
            "mnd",
            "lll",
            "exhaustruct",
            "funlen",
            "gocognit",
            "cyclop",
            "maintidx",
            "dupl",
            "ireturn",
            "nilnil",
            "testableexamples",
            "depguard",
            "varnamelen",
            "noinlineerr",
            "wsl"
        ]
    },
    "issues": {
        "fix": false,
        "max-issues-per-linter": 0,
        "max-same-issues": 0
    }
}